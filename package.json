    "thead",
    "browser"
  ],
  "author": "_X_y_z_",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunya9/fixed-thead/issues"
  },
  "homepage": "https://github.com/sunya9/fixed-thead#readme",
  "devDependencies": {
    "eslint": "^2.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-jasmine": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "uglify-js": "^2.6.1"
  }
